{
  "modelName": "Patient",
  "action": "updateOne",
  "query": {
    "arguments": {
      "relationLoadStrategy": "query",
      "where": { "id": 1 },
      "data": {
        "user": {
          "update": {
            "notificationSettings": {
              "update": { "emailMarketing": true, "smsMarketing": false }
            }
          }
        }
      }
    },
    "selection": {
      "$scalars": true
    }
  }
}
