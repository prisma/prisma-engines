// tags=postgres
// exclude=cockroachdb

datasource mydb {
    provider = "postgresql"
}

model MyTable {
    id Int @id
    a Activity @default(SLEEPING)
    k Kind? @default(error)
    r Result @default(succeeded)
}

enum Result {
  succeeded
  failed
}

enum Activity {
  DANCING
  SLEEPING
  READING
}

enum Kind {
  info
  warning
  error
}

