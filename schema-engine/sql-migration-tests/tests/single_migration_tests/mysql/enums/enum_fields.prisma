// tags=mysql

datasource mydb {
    provider = "mysql"
    url = "dummy-url"
}

model MyTable {
    id Int @id
    k Kind? @default(error)
    r Result @default(succeeded)
}

enum Result {
  succeeded
  failed
}

enum Kind {
  info
  warning
  error
}
