datasource db {
  provider = "mongodb"
}

type Embed {
  foo Int @map("bar")
}

model User {
  id        Int    @id @map("_id")
  firstName String @map("fst_name")
  lastName  String @map("lst_name")
  age       Int
  embed     Embed

  @@index([firstName, lastName, age, embed.foo])
  @@map("users")
}
