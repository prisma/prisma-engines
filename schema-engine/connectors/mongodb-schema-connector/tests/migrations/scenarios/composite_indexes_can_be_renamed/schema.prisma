datasource db {
  provider = "mongodb"
  url      = "mongodb://"
}

generator js {
  provider        = "prisma-client"
  previewFeatures = []
}

type Address {
  street String
  number Int
}

model User {
  id      Int     @id @map("_id")
  address Address

  @@index([address.number], map: "meow")
}
