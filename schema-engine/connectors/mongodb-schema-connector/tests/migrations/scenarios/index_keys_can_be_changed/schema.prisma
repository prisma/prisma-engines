datasource db {
  provider = "mongodb"
}

type Address {
  number Int
}

model User {
  id        Int     @id @map("_id")
  firstName String
  lastName  String
  age       Int
  address   Address

  // changing the order, here
  @@index([lastName, firstName, age, address.number], map: "UserExistingIndex")
}

model Post {
  id    Int    @id @map("_id")
  title String
}
