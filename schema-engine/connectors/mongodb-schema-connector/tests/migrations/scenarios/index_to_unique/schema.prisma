datasource db {
  provider = "mongodb"
}

type Ip {
  address String
}

model User {
  id        Int    @id @map("_id")
  firstName String
  lastName  String
  age       Int
  ip        Ip

  @@unique([firstName, lastName, age, ip.address], map: "UserExistingIndex")
}

model Post {
  id    Int    @id @map("_id")
  title String
  ip    Ip

  @@index([title, ip.address])
}
