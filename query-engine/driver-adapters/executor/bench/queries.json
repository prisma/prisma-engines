        "query": {
            "modelName": "Movie",
            "action": "findMany",
            "query": {
                "arguments": {                
                    "where": {
                        "cast": {
                            "some": {
                                "person": {
                                    "OR": [
                                        {
                                            "last_name": { "gt": "a" }
                                        },
                                        {
                                            "last_name": { "lt": "f" }
                                        }
                                    ]
                                }
                            }
                        }
                    },
