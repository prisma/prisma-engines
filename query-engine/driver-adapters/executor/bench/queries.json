    {
        "description": "movie.findMany({ where: { reviews: { author: { ... } }, take: 100 }) (to-many -> to-one)",
        "query": {
            "modelName": "Movie",
            "action": "findMany",
            "query": {
                "arguments": {                
                     "where": {
                        "reviews": {
                            "some": {
                                "author": {
                                    "OR": [{ "name": { "gt": "a" } }, { "name": { "lt": "f" } }]
                                }
                            }
                        }
                    },
                    "take": 100
                },
                "selection": {
                    "$scalars": true
                }
            }   
        }
    },
    {
