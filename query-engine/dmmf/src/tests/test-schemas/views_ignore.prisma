generator client {
  provider        = "prisma-client"
  output          = "../node_modules/.prisma/client"
  previewFeatures = ["views"]
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model User {
  id    String  @id @default(uuid())
}

view IgnoredView {
  id String

  @@ignore
}
