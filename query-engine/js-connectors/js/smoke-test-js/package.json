    "@jkomyno/prisma-neon-js-connector": "workspace:*",
    "@jkomyno/prisma-planetscale-js-connector": "workspace:*",
    "@prisma/client": "5.1.0"
  },
  "devDependencies": {
    "prisma": "^5.1.0",
    "tsx": "^3.12.7"
  }
