datasource mydb {
  provider = "sqlserver"
  url = env("TEST_DB_URL")
  schemas  = ["base", "transactional"]
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}


model Dog {
  id Int @id

  @@map("pets")
  @@schema("base")
}

model Cat {
  id Int @id(map: "cat_pets_pkey")

  @@map("pets")
  @@schema("base")
}

