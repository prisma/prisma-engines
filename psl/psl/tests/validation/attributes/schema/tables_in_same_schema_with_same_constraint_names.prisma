generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

datasource db {
    provider = "postgresql"
    url      = env("TEST_DATABASE_URL")
    schemas  = ["base", "transactional"]
}

model User {
    id    String @id @default(cuid())
    email String
    posts Post[]

    @@map("some_table")
    @@schema("transactional")
}

model Post {
    id       String @id @default(cuid())
    title    String
    authorId String
    author   User?  @relation(fields: [authorId], references: [id])

    @@map("some_table")
    @@schema("transactional")
}

