datasource testds {
    provider = "postgresql"
    url = env("TEST_DATABASE_URL")
    schemas = ["public", "private"]
}

generator js {
    provider = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

model Test {
  id Int @id
  @@schema("public")
}

enum toggle {
  on
  off

  @@schema("public")
  @@schema("private")
}



