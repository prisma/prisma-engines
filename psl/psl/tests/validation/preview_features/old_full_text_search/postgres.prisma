generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Blog {
  id      Int    @unique
  content String
  title   String
  @@index([content, title])
}
