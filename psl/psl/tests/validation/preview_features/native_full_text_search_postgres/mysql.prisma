generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["nativeFullTextSearchPostgres"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Blog {
  id      Int    @unique
  content String
  title   String
  @@fulltext([content, title])
}
