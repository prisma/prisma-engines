generator client {
  provider = "prisma-client"
  output = "../lib/.generated/prisma"
}

generator edge {
  provider = "prisma-client"
  output = "../lib/.generated/prisma-edge"
  runtime = "edge-light"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
