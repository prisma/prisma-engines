generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = "mysql://"
    relationMode = "foreignKeys"
}

model A {
    id Int @id
    bs B[]
}

model B {
    id Int   @id
    aId Int? @default(3)
    a   A?   @relation(fields: [aId], references: [id], onUpdate: SetDefault, onDelete: SetDefault)
}
