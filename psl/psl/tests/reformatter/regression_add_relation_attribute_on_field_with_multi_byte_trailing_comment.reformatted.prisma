generator client {
  provider = "prisma-client"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlserver"
}

model somemodel {
  frownyFaceId String
  frownyFace   frownyface @relation(fields: [], references: []) // å›§

  id Int @id
}

model frownyface {
