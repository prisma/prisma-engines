generator client {
    provider = "prisma-client"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider = "mysql"
    relationMode = "prisma"
}

model SomeUser {
    id Int @id
    posts Post[]
}

model Post {
    id Int @id
    userId Int
    user SomeUser @relation(fields: [userId], references: [id])
}
