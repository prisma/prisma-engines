generator client {
    provider = "prisma-client"
}

datasource db {
    provider     = "mysql"
    relationMode = "foreignKeys"
}

/// multi line
/// commennttt
model Foo {
    id     Int  @id
    bar    Bar  @relation(fields: [bar_id], references: [id], onUpdate: SetDefault)
    bar_id Int  @unique
    t      Test
}

model Bar {
    id  Int  @id
    foo Foo?
}

// This is a test enum.
enum Test {
    TestUno
    TestDue
}
