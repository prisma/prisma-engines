generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

datasource db {
    provider = "postgresql"
    url      = env("TEST_DATABASE_URL")
    schemas  = ["one", "two"]
}

model User {
    id Int @id
}

model Post {
    id Int @id

    @@schema("two")
}
