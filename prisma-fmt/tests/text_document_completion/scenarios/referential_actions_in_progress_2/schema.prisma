datasource db {
    provider = "postgresql"
    url      = env("TEST_DATABASE_URL")
}

model Test {
    id Int @id
}

model Dog {
    id         Int    @id
    name       String
    age        Int
    bestFriend Cat    @relation(fields: [catId], references: [id], onDelete: Re<|>)
    catId      Int    @unique
}

model Cat {
    id         Int     @id
    name       String
    age        Int
    bites      Boolean
    bestFriend Dog?    @relation
}

