datasource testds {
    provider = "postgresql"
    url = env("TEST_DATABASE_URL")
    schemas = ["public"]
}

generator js {
    provider = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

model Test {
  id Int @id
  @@schema("public")
}

enum Language {
  English
  Spanish

  @@schema("public")
}


