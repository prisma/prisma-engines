datasource db {
  provider = "mongodb"
  url      = "mongodb://"
}

generator js {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

type Address {
  street String
  number Int
}

model User {
  id      Int     @id @map("_id")
  address Address

  @@index([address.number])
}
